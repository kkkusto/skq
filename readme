SELECT
    FORMAT(row_insert_time, 'yyyy-MM') MonthYear,
    COUNT(*) RowCount,
    SUM(COUNT(*)) OVER (ORDER BY FORMAT(row_insert_time, 'yyyy-MM')) CumulativeRowCount
FROM your_table_name
WHERE row_insert_time >= DATEADD(YEAR, -5, GETDATE())
GROUP BY FORMAT(row_insert_time, 'yyyy-MM')
ORDER BY MonthYear;
